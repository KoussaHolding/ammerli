{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "vndevteam",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "env-cmd typeorm-ts-node-commonjs -d src/database/data-source.ts",
    "migration:up": "pnpm typeorm migration:run",
    "migration:down": "pnpm typeorm migration:revert",
    "migration:show": "pnpm typeorm migration:show",
    "migration:create": "typeorm migration:create",
    "migration:generate": "pnpm typeorm migration:generate --pretty",
    "db:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:create",
    "db:drop": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:drop",
    "seed:run": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run",
    "seed:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create",
    "prepare": "husky || true",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "test:e2e:sqlite": "jest --config ./test/jest-e2e.json --setupFiles ./test/setup-sqlite.ts"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^7.1.1",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "@socket.io/redis-adapter": "^8.3.0",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.66.4",
    "cache-manager": "^7.2.7",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "firebase-admin": "^13.6.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.6.0",
    "nestjs-pino": "^4.5.0",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.28",
    "typeorm-extension": "^3.7.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/redlock": "^4.0.8",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@types/winston": "^2.4.4",
    "env-cmd": "^11.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "sass-embedded": "^1.97.1",
    "socket.io-client": "^4.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
